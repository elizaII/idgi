<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".android.fragment.CourseUserListFragment">

    <com.idgi.android.recyclerview.EmptyRecyclerView
        android:id="@+id/course_user_list_recycler_view"
        android:scrollbars="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <include layout="@layout/list_view_empty"
        android:id="@+id/course_user_list_empty_view"/>

</FrameLayout>
